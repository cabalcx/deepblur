{

  "description": "Blurs out images on a webpage so researchers can focus on content and navigation.",
  "manifest_version": 2,
  "name": "deepblur",
  "version": "1.0",
  "homepage_url": "https://github.com/cabalcx/deepblur",
  "icons": {
    "48": "icons/icon-48.png",
    "64": "icons/icon-64.png",
    "128": "icons/icon-128.png",
    "500": "icons/icon-500.png"
  },
  "browser_action": {
    "default_icon": "icons/icon-48.png",
    "theme_icons": [{
        "light": "icons/icon-48.png",
        "dark": "icons/icon-48.png",
        "size": 32
    }],
    "default_title": "deepblur",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true,
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]

}
